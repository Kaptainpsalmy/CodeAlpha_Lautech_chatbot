{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel_handler.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/vercel_handler.py"
    },
    {
      "src": "/",
      "dest": "api/vercel_handler.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}